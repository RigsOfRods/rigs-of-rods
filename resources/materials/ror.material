	set_texture_alias diffuse_tex chp.dds
}


material tracks/netchat2/bg/left
{
    technique
    {
        pass
        {
			lighting off
			depth_check off
            scene_blend alpha_blend
            texture_unit
            {
                colour_op_ex source1 src_manual src_current 0.3 0.3 0.3
                alpha_op_ex source1 src_manual src_current 0.7
            }
        } 
    }
}


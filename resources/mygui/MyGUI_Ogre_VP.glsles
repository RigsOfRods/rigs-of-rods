#version 100

precision highp int;
precision highp float;

attribute vec4 position;
attribute vec4 uv0;
attribute vec4 colour;
uniform mat4 worldViewProj;

varying vec4 outUV0;
varying vec4 outColor;

// Texturing vertex program for GLSL ES
void main()
{
	gl_Position = worldViewProj * position;
	outUV0 = uv0;
	outColor = colour;
}

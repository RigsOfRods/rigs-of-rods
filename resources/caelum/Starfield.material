material CaelumStarfieldMaterial
{
	receive_shadows off
	
	technique
	{
		pass
		{
			depth_check off
			depth_write off
			lighting off
			fog_override true
			
			texture_unit
			{
				texture Starfield.jpg 0
				tex_address_mode wrap
			}
		}
	}
}

